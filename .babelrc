{
  "presets": [
    "react",
    [
      "env",
      {
        "modules": "commonjs",
        "loose": false,
        "targets": {
          "node": "current",
        }
      }
    ]
  ],
  "plugins": [
    [
      "module-resolver",
      {
        "root": [
          "./src"
        ]
      }
    ],
    ["styled-components", {
      "ssr": true,
    }],
    "loadable-components/babel",
    "dynamic-import-node",
    "transform-object-rest-spread",
    "transform-class-properties"
  ]
}
